		<ul>
			<li><a href="{{path('sym_index')}}">Inicio</a></li>
			<li><a href="{{path('sym_index1')}}">Prueba1</a></li>
            <li><a href="{{path('sym_index2')}}">Prueba2</a></li>
		</ul>
	</nav>

	{% if nombre is defined %}
		<p>Hola
			{{ nombre }}!!</p>
		<p>{{ saludo }}!!</p>
