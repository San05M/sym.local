
	{% extends 'base.html.twig' %}
	
	{% block title %}
		{% if nombre is defined %}
			<p>Hola
				{{ nombre }}!!</p>
		{% else %}
			<p>Hola mundo !!</p>
		{% endif %}
		<p>
			Estamos probando
		</p>
		<p>{{ saludo }}!!</p>

		<ul>
			{% for nombre in nombres %}
				<li>{{ nombre }}</li>
			{% endfor %}
		</ul>
		<p>{{ fecha | date }}</p>
{% endblock %}
